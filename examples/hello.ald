# This is a simple infinite hello world program

# MAIN

START:
        MOV CX 00
LOOP:
        PRINTCHAR [TEXT+CX]B
        ADD CX CX 01
        JLT CX [TExt_LENGTH]B LOOP
        JUMP START

# DATA

TEXT:
        DAT 'Hello'
        DAT 20  # space
        DAT 'world'

TEXT_LENGTH:
        DAT 0B
